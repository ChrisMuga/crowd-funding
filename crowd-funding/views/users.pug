extends layout
block content
    div( class = "alert alert-warning my-2") Users
    div( class = "list-group" )
        - counter = 1
        each user in users     
            a(href="#" class="list-group-item list-group-item-action" ) 
                span( class = "badge badge-primary mr-3" ) #{ counter }
                span #{ user.name } 
                -counter ++